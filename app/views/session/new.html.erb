<%= form_tag('/session', method: :post) do %>
<%= label_tag(:name, "Your Name") %>
<%= collection_select(:user, :name, User.all, :name, :name, prompt: "Please select a name") %>
<%= label_tag(:password, "Your Password") %>
<%= password_field_tag(:password, nil) %>
<%= submit_tag("Sign In") %>
<% end %>
